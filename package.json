    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "mocha": "^4.0.1",
    "realm-object-server": "^2.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "apollo-server-express": "^1.2.0",
    "graphql": "^0.11.7",
    "graphql-subscriptions": "^0.5.5",
    "graphql-tools": "^2.10.0",
    "lru-cache": "^4.1.1",
    "pluralize": "^7.0.0",
    "reconnecting-websocket": "^3.2.2",
    "subscriptions-transport-ws": "^0.9.1"
