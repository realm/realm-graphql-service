  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/faker": "^4.1.2",
    "@types/graphql": "^0.11.6",
    "@types/lru-cache": "^4.1.0",
    "@types/mocha": "^2.2.44",
    "@types/pluralize": "0.0.28",
    "@types/reconnectingwebsocket": "^1.0.1",
