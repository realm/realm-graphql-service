{
    "extends": [
      "tslint:latest",
      "tslint-eslint-rules"
    ],
    "rules": {
      "object-literal-sort-keys": false,
      "jsx-boolean-value": false,
      "jsx-no-lambda": false,
      "max-line-length": false,
      "no-submodule-imports": [
        true,
        "graphql-tools"
      ],
      "no-implicit-dependencies": false,
      "quotemark": [
        true,
        "double",
        "avoid-escape"
      ],
      "trailing-comma": [true, {
        "multiline": "always",
        "singleline": "never",
        "esSpecCompliant": true
      }],
      "interface-name": [
        true,
        "never-prefix"
      ]
    }
  }
