{
    "extends": [
      "tslint:latest",
      "tslint-eslint-rules",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "object-literal-sort-keys": false,
      "jsx-boolean-value": false,
      "jsx-no-lambda": false,
      "no-submodule-imports": [
        true
      ],
      "no-implicit-dependencies": [
        true,
        "dev"
      ],
      "prettier": [
        true,
        {
          "singleQuote": true,
          "trailingComma": "all"
        }
      ],
      "indent": [true, "spaces", 4]
    }
  }
  